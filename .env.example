# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control (add to .gitignore)

# Payment Configuration
REACT_APP_UPI_ID=your-upi-id@bank
REACT_APP_MERCHANT_NAME=Thiruchendur Murugan Restaurant

# Security Configuration
REACT_APP_HTTPS_ENABLED=true
REACT_APP_CSP_ENABLED=true
REACT_APP_HSTS_MAX_AGE=31536000

# Logging Configuration
REACT_APP_ENABLE_LOGS=true
REACT_APP_LOG_LEVEL=info

